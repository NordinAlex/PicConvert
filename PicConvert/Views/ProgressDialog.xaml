<ContentDialog
    x:Class="PicConvert.Views.ProgressDialog"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="Konvertering pågår"
    PrimaryButtonText="Avbryt"
    PrimaryButtonClick="OnPrimaryButtonClick">
    <StackPanel>
        <ProgressBar x:Name="ConversionProgressBar" IsIndeterminate="False" Minimum="0" Maximum="100" Height="20"/>
        <TextBlock x:Name="ConversionStatusText" Text="Startar..." Margin="0,10,0,0"/>
    </StackPanel>
</ContentDialog>
